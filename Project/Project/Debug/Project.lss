                                 ;Macros-------------------------------------------------------------------
                                 
                                 .macro clear
                                 	ldi YL, low(@0)     ; load the memory address to Y
                                     ldi YH, high(@0)
                                     clr temp1 
                                     st Y+, temp1        ; clear the two bytes at @0 in SRAM
                                     st Y, temp1
                                 .endmacro
                                 
